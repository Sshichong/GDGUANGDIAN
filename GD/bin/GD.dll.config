<configuration>
  <!--日志start-->
  <configSections>
        <section name="log4net" type="System.Configuration.IgnoreSectionHandler" />
    </configSections>
    <log4net>
    <appender name="RollingLogFileAppender" type="log4net.Appender.RollingFileAppender">
      <file value="D:/myp2c_node-log.txt" />
      <appendToFile value="true" />
      <rollingStyle value="Composite" />
      <datePattern value="yyyyMMdd" />
      <maxSizeRollBackups value="10" />
      <maximumFileSize value="1MB" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date [%thread] %-5level %logger [%property{NDC}] - %message%newline" />
      </layout>
    </appender>
    <root>
      <level value="ALL" />
      <appender-ref ref="RollingLogFileAppender" />
    </root>
  </log4net>
<!--日志end-->

  <appSettings>
    <add key="webpages:Version" value="3.0.0.0" />
    <add key="webpages:Enabled" value="false" />
    <add key="ClientValidationEnabled" value="true" />
    <add key="UnobtrusiveJavaScriptEnabled" value="true" />
    <add key="mysqlConnection" value="Database='gd';Data Source='192.1.2.170';User Id='root';Password='root';charset='utf8';pooling=true" />
    <!--<add key="sqlserverConnection" value="server ='192.1.2.210';uid = 'sa';pwd = '123456'; database = 'EQM_BJ'"/>-->
    <!--<add key="mysqlConnection" value="Database='ytrm_daobo';Data Source='192.1.2.190';User Id='mysqldev';Password='mysqldev';charset='utf8';pooling=true" />-->
    <add key="sqlserverConnection" value="server ='192.1.2.210';uid = 'sa';pwd = '123456'; database = 'EQM_BJXC'" />
  </appSettings>
